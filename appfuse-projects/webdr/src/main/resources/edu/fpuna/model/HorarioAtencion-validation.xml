<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="horarioAtencion.horaInicio">
       <field-validator type="conversion">
          <message>La hora ingresada tiene un formato incorrecto</message>
       </field-validator>
		<field-validator type="required">
				<message key="errors.required"/>
		</field-validator>
	</field>
	<field name="horarioAtencion.horaFin">
       <field-validator type="conversion">
          <message>La hora ingresada tiene un formato incorrecto</message>
       </field-validator>
	   <field-validator type="required">
			<message key="errors.required"/>
		</field-validator>
	</field>
</validators>
